#!/bin/sh

# This file runs when a DM logs you into a graphical session.
# If you use startx/xinit like a Chad, this file will also be sourced.

feh --bg-scale ~/wallpaper/wall_5.jpg # setting background
compton --config ~/.config/compton/config --vsync opengl --mark-wmwin-focused --mark-ovredir-focused -b # for window blur effect
slstatus &		# script for updating the status bar
dunst &			# dunst for notifications
sxhkd &			# Bind keys with sxhkd
xset r rate 300 50 &	# Speed xrate up
unclutter &		# Remove mouse when idle
xrdb ~/.Xresources # update x database
nordvpn connect georgia &
i8kmon &

# xrandr --dpi 138 &

notify-send "Welcome to Rice of RAFASAURUS"
