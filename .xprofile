#!/bin/sh

# This file runs when a DM logs you into a graphical session.

# feh --bg-scale ~/wallpaper/wall_5.jpg
xwallpaper --zoom ~/.config/wall.png
compton --config ~/.config/compton/config --vsync opengl --mark-wmwin-focused --mark-ovredir-focused -b --xrender-sync --xrender-sync-fence
# compton -CGb
slstatus &		# script for updating the status bar
dunst &			# dunst for notifications
logid &
xset r rate 300 50 &	# Speed xrate up
xmodmap $HOME/.Xmodmap &
unclutter &		# Remove mouse when idle
xrdb ~/.cache/wal/.Xresources # update x database
nordvpn connect georgia &
i8kmon &
bluetoothctl power on &
i8kfan - 0 &
sxhkd &			# Bind keys with sxhkd
# dropbox &

xrandr --dpi 115 &
if type chromium >/dev/null 2>/dev/null
then
	export BROWSER=chromium
fi

export BROWSER=chromium
notify-send "Welcome to Rice of RAFASAURUS"
