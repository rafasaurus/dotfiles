[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[core]
s0_active_plugins = core;composite;opengl;place;grid;regex;mousepoll;vpswitch;imgpng;session;snap;compiztoolbox;wall;resize;move;animation;expo;ezoom;staticswitcher;workarounds;fade;scale;unityshell;
s0_outputs = 1920x1080+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_minimize_window_key = <Super>j
s0_maximize_window_key = <Super>Up
s0_unmaximize_or_minimize_window_key = <Super>Down
s0_show_desktop_key = <Control><Super>d
s0_toggle_window_maximized_key = <Super>k
s0_hsize = 5

[expo]
s0_x_offset = 40
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[mousepoll]
s0_mouse_poll_interval = 1

[vpswitch]
s0_switch_to_1_key = <Super>Home

[staticswitcher]
s0_next_key = Disabled
s0_prev_key = Disabled
s0_background_color = #333333d9

[wall]
s0_show_switcher = false
s0_miniscreen = false
s0_preview_timeout = 0.400000
s0_border_width = 0
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
