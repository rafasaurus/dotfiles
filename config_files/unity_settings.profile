s0_right_key = <Alt>2
s0_left_window_key = <Alt>3
s0_right_window_key = <Alt>4

[scale]
s0_x_offset = 40

[opengl]
s0_texture_filter = 0
s0_lighting = true
s0_texture_compression = true

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = st
s0_run_command_terminal_key = <Control><Alt>t

[grid]
s0_put_center_key = <Super>c
s0_put_left_key = <Super>h
s0_put_right_key = <Super>l
s0_put_maximize_key = <Super>k
s0_right_maximize = <Super>l
s0_snapback_windows = false
s0_cycle_sizes = true

[animation]
s0_open_effects = animation:None;animation:Zoom;animation:Zoom;
s0_open_durations = 100;100;100;
s0_open_matches = ((type=Normal | Unknown) | name=sun-awt-X11-XFramePeer | name=sun-awt-X11-XDialogPeer) & !(role=toolTipTip | role=qtooltip_label) & !(type=Normal & override_redirect=1) & !(name=gnome-screensaver);((type=Menu | PopupMenu | DropdownMenu | Combo | Dialog | ModalDialog | Normal) & !(class=\.exe$));(type=Tooltip | Notification | Utility) & !(name=compiz) & !(title=notify-osd);
s0_close_effects = animation:None;animation:None;animation:None;
s0_close_durations = 150;100;50;
s0_close_matches = ((type=Normal | Unknown) | name=sun-awt-X11-XFramePeer | name=sun-awt-X11-XDialogPeer) & !(role=toolTipTip | role=qtooltip_label) & !(type=Normal & override_redirect=1) & !(name=gnome-screensaver) & !(name=gnome-screenshot);((type=Menu | PopupMenu | DropdownMenu | Combo | Dialog | ModalDialog | Normal) & !(class=\.exe$));(type=Tooltip | Notification | Utility) & !(name=compiz) & !(title=notify-osd);
s0_minimize_effects = animation:None;
s0_minimize_durations = 120;
s0_time_step = 10
s0_curved_fold_zoom_to_taskbar = false
s0_magic_lamp_moving_end = false
s0_magic_lamp_open_start_width = 10

[switcher]
s0_opacity = 100
s0_bring_to_front = false
s0_zoom = 0.000000
s0_icon = false
s0_background_color = #333333d9

[unityshell]
s0_lock_screen = Disabled
s0_show_hud = <Alt>slash
s0_dash_blur_experimental = 0
s0_override_decoration_theme = true
s0_active_shadow_radius = 30
s0_active_shadow_color = #000000ff
s0_inactive_shadow_radius = 30
s0_inactive_shadow_color = #0000007c
s0_show_launcher = <Super>Return
s0_launcher_switcher_forward = Disabled
s0_launcher_opacity = 1.000000
s0_launcher_hide_mode = 1
s0_autohide_animation = 2
s0_edge_responsiveness = 0.200000
s0_icon_size = 16
s0_alt_tab_bias_viewport = false

